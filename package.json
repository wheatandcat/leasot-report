{
  "name": "leasot-report",
  "version": "0.7.2",
  "description": "leasot output report",
  "main": "index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:init": "rm -rf dist && mkdir dist",
    "build:js": "babel src --out-dir dist",
    "build:install":
      "cp package.json dist/ && cd dist && yarn install --production",
    "build:chmod": "chmod -R 777 dist/*.js",
    "start": "babel-node src/index.js",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "bin": {
    "leasot-report": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheatandcat/leasot-report.git"
  },
  "author": "wheatandcat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wheatandcat/leasot-report/issues"
  },
  "homepage": "https://github.com/wheatandcat/leasot-report#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.11",
    "@storybook/addon-knobs": "^3.2.10",
    "@storybook/addon-options": "^3.2.10",
    "@storybook/cli": "^3.2.10",
    "@storybook/react": "^3.2.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-node": "^5.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.7.2",
    "flow-bin": "^0.55.0",
    "jest": "^21.1.0",
    "npm-run-all": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "leasot": "^4.10.1",
    "mkdirp": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2"
  }
}
